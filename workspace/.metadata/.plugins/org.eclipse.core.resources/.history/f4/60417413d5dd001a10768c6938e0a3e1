package com.pc.oop.polymorphism.override;

public class Test {

	public static void main(String[] args) {
		Animal a = new Animal();
		a.eating();
		a.running();
		
       Dog d = new Dog();
       d.eating();
       d.running();
       d.sounds();
       
       Cat c = new Cat();
       c.eating();
       c.running();
       
       //Dynamic dispatch
       Animal a1 = new Dog();
       a1.eating();
       a1.running();
       
       
       

	}

}
